{
  "name": "blok/repository",
  "description": "A repository",
  "license": "MIT",
  "keywords": [
    "repository"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Daniel Sum",
      "email": "daniel@cherrypulp.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.5",
    "blok/utils" : "~5.5"
  },
  "require-dev": {
    "orchestra/testbench": "~3.6.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Blok\\Repository\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Blok\\Repository\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Blok\\Repository\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
